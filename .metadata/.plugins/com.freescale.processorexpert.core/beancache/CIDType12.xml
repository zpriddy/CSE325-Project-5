<?xml version="1.0" encoding="UTF-8"?>
<Bean>
  <Header>
    <Name>CIDType12</Name>
    <Description>The Type 1 and 2 Telephony Features Library</Description>
    <Author>Processor Expert / ACh</Author>
    <Version>01.013</Version>
    <Icon>TELEPHONE</Icon>
    <TypesFiles>PE,CIDType12\CIDType12</TypesFiles>
    <FileVersion>6</FileVersion>
  </Header>
  <Options>
    <BW_HelpType>Default</BW_HelpType>
    <BW_HelpFiles>,Properties,Methods,Events,"User Types"</BW_HelpFiles>
    <BW_AutoSaveHelp>yes</BW_AutoSaveHelp>
    <BW_AutoSaveDriver>yes</BW_AutoSaveDriver>
    <BW_DetailedHelp>yes</BW_DetailedHelp>
    <BW_NeedTps>no</BW_NeedTps>
    <Licence>CID12</Licence>
    <BeanStatus>RELEASE</BeanStatus>
    <Category>SW-Telephony Library</Category>
    <GroupCategory>SDK</GroupCategory>
    <Shortcut>CIDx</Shortcut>
